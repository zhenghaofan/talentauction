.side-r {
	float: left;
	width: 747px;
	padding-bottom: 2px;
}
.search-warp {
    margin-bottom: 20px;
}
.search-warp .tab-item {
	font-size: 12px;
	padding: 5px 6px 3px 8px;
	display: inline-block;
	cursor: pointer;
} 
.search-warp .tab-item:hover {
	color: #3578bc;
}
.search-warp .tab-item.slt {
	color: #fff;
	background-color: #2eb8ea;
}
.search-warp .search-item.inp-box {
    border-radius: 3px;
    overflow: hidden;
}
.search-warp input {
    width: 645px;
    height: 38px;
    color: #6B7485;
    font-size: 14px;
    text-indent: 12px;
    border-radius: 0;
    border: 0;
    display: inline-block;
    background-color: #fff;
    border: 2px solid #2eb8ea;
    border-right: 0px;
}
.search-warp .search-btn {
    float: right;
    width: 100px;
    height: 42px;
    color: #fff;
    font-size: 18px;
    vertical-align: bottom;
    border: 0;
    background-color: #2eb8ea;
    cursor: pointer;
    border: 2px solid #2eb8ea;
}
.search-warp .search-btn:hover {
	background-color: #13a5da;
	border: 2px solid #13a5da;
}
.search-warp .search-item {
    margin-bottom: 10px;
}
.search-warp .words {
    margin-right: 20px;
    cursor: pointer;
}
.search-warp .words:hover {
	color: #568cca;
}



