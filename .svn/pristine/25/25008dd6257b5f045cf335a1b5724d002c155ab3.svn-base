.list-box .title-l, 
.list-box .info-l {
    width: 46%;
    font-size: 16px;
    margin: 0px 5px 10px 45px;
    display: inline-block;
}
.list-box .title-r, 
.list-box .info-r {
    width: 40%;
    margin: 0px 35px 10px 5px;
    display: inline-block;
}
.list-box .info-l, 
.list-box .info-r {
    font-size: 14px;
    line-height: 26px;
}
.list-box .title-l .ellipsis {
    min-width: 50px;
    max-width: 205px;
    text-overflow: ellipsis;
    white-space: nowrap;
    vertical-align: middle;
    overflow: hidden;
    display: inline-block;
}
.list-box .ring-tag {
    max-width: 140px;
    color: #71a8e0;
    font-size: 12px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    padding: 2.5px 6px;
    border: 1px solid #71a8e0;
    margin-right: 10px;
}
.list-box .no-data {
    height: 180px;
    color: #616268;
    padding-top: 100px;
    text-align: center;
    background-color: #fff;
}

.screen-box {
	width: 689px;
	height: 229px;
	border: 1px solid #eceef1;
	margin: 20px 29px 10px;
}
.screen-box .tr {
	height: 45px;
	line-height: 45px;
	padding: 0 20px;
	border-bottom: 1px solid #eceef1;
}
.screen-box .text-box {
	display: inline-block;
}
.screen-box .text-item {
	height: 24px;
	line-height: 24px;
	border-radius: 0px;
	margin-left: 40px;
	display: inline-block;
	cursor: pointer;
}
.screen-box .text-item:first-child {
	margin-left: 0;
}
.screen-box .text-item:hover {
	color: #3578bc;
}
.screen-box .text-item.slt {
	color: #fff;
	padding: 0 8px;
	background-color: #2eb8ea;
}

/****** 充值弹出框******/
.guide-modal {
	display: none;
}
.guide-modal .modal-dialog {
	width: 750px;
	position: absolute;
    top: 26%;
    left: 30%;
    margin: 0 auto;
}
.guide-modal .close {
	font-size: 12px;
    color: #3578bc;
    float: right;
    cursor: pointer;
    opacity: inherit;
    margin-top: auto;
    line-height: inherit;
    font-weight: inherit;
}
.guide-modal .close:hover{
    color: #72a6dc;
}
.guide-modal .modal-content {
	color: #6B7485;
	border-radius: 2px;
}
.pay-box {
    padding: 10px 0px 15px;
    background-color: #fff;
}
.pay-box .title {
    height: 75px;
    font-size: 26px;
    line-height: 85px;
    padding: 0 45px;
    color: #4e5663;
    border-bottom: 1px solid #eceef1;
    background-color: #fff;
}
.body{
    padding: 45px 100px;
}
.pay-box .pkg-box {
	margin-bottom: 10px;
    float: left;
    
}
.pay-box .pkg-box .pkg-item {
	position: relative;
    height: 40px;
    line-height: 40px;
    text-align: center;
    padding: 0 18px;
    margin-right: 10px;
    border: 2px solid #fff;
    border-radius: 0px;
    background-color: #f7f7f7;
    display: inline-block;
    cursor: pointer;
    margin-bottom: 20px;
    width: 174px;
}
.pkg-box .pkg-item.slt {
	border: 2px solid #44b9e3;
	
}
.pkg-box .pkg-item.slt:after {
	content: '';
	position: absolute;
	top: 15px;
    right: -1px;
    width: 25px;
    height: 25px;
    background: url("../img/icon-check-d.png") no-repeat;
    display: inline-block;
}

.info-text-pice{
    float: right;
    width: 45%;
    margin-right: -50px;
}

.pay-box .info-text > span {
	height: 35px;
	line-height: 35px;
	display: block;
}


.pay-box .btn-group {
	margin: 20px 0 50px;
}
.pay-box .btn-group .btn-pay {
    padding: 9.5px 30px;
    background: url("../img/ico_user_btn.png") no-repeat -26px -175px;
    border: 1px solid transparent;
    border-radius: 1px;
    text-shadow: -1px -1px 0px #3195b9;
    color: #fff;
    margin-right: 100px;
    cursor: pointer;
}
.pay-box .btn-group .btn-pay:hover {
     background-position: -136px -175px;
}
.pay-box .tip-wrap {
	font-size: 12px;
    color: #aaa;
    margin-top: 10px;
}
.pay-box .tip-wrap span {
	min-height: 22px;
	line-height: 22px;
	display: block;
}

