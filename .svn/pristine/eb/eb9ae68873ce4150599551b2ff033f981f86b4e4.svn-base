/****** 引导页******/
.guide-modal {
	display: none;
}
.guide-modal .modal-dialog {
	width: 500px;
	text-align: center;
	margin-top: 10%;
}
.guide-modal .modal-content {
	color: #6B7485;
	border-radius: 0;
	border: 0;
	box-shadow: 0px 0px;
	-webkit-box-shadow: 0px 0px;
	background-color: transparent;
}
.guide-modal .modal-content img {
	width: 480px;
}
.guide-modal .modal-body {
	padding: 0 15px;
	overflow: hidden;
}
.red-close {
	position: absolute;
	top: 0;
  	right: 5px;
	width: 38px;
	height: 38px;
	cursor: pointer;
}
.now-sign {
	position: absolute;
	bottom: 70px;
  	left: 116px;
	width: 285px;
	height: 30px;
	transition: opacity 0.5s linear 0s;
	display: inline-block;
	cursor: pointer;
}
.now-sign {
	-moz-animation:bg 1.2s ease-in-out;
	-webkit-animation:bg 1.2s ease-in-out;
}
@-moz-keyframes bg{0%{background:rgba(144,24,53,0.18);}
	25%{background:rgba(144,24,53,0.5);}
	50%{background:rgba(144,24,53,0.1);}
	75%{background:rgba(144,24,53,0.5);}
	100%{background:rgba(144,24,53,0.18);}
} 
@-webkit-keyframes bg{0%{background:rgba(144,24,53,0.18);}
	25%{background:rgba(144,24,53,0.5);}
	50%{background:rgba(144,24,53,0.1);}
	75%{background:rgba(144,24,53,0.3);}
	100%{background:rgba(144,24,53,0.18);}
} 









