

body {
	background: #fff;
}
.index-wrap {
	color: #6B7485;
	margin: 46px auto 0;
}
.index-item {
}


/****** 首页引导1******/
.guide1-wrap {
	height: 295px;
	color: #2d4e7a;
	text-align: center;
	margin: 0 auto;
	background: url(../img/index_banner.png) no-repeat;
	display: block;
}
.guide1-wrap .content {
	width: 1024px;
	padding-top: 70px;
	margin: 0 auto;
}
.guide1-wrap .text-1 {
	color: #2d4e7a;
	font-size: 30px;
	text-align: center;
	margin-bottom: 5px;
}
.guide1-wrap .text-2 {
	color: #2d4e7a;
	font-size: 18px;
	text-align: center;
	margin-top: 20px;
}
.guide1-wrap .text-3 {
	position: relative;
	top: 65px;
}
.guide1-wrap .text-3 a {
	height: 50px;
	width: 215px;
	color: #fff;
	font-size: 20px;
	line-height: 50px;
	background: #48bde7;
	display: inline-block;
}
.guide1-wrap .text-3 a:hover{
	background-color: #4182c6;
}

/****** 首页引导2******/
.guide2-wrap {
	min-height: 100px;
	text-align: center;
	background-color: #fff;
}
.guide2-wrap .hot-con {
	width: 1024px;
	padding-top: 60px;
	margin: 0 auto;
}
.guide2-wrap .hot-item {
	position: relative;
	width: 110px;
	height: 110px;
	overflow: hidden;
	margin: 0 28px;
	display: inline-block;
}
.guide2-wrap .img-box {
	width: 110px;
	height: 110px;
	text-align: center;
	line-height: 90px;
}
.guide2-wrap .hot-img {
	max-width: 110px;
	max-height: 110px;
	vertical-align: middle;
	display: inline-block;
}
.guide2-wrap .hot-box-bg {
	position: absolute;
	top: 0;
	left: 0;
	width: 110px;
	height: 110px;
	color: #fff;
	text-align: center;
	background-color: rgba(60, 110, 177, 0.81);
	display: none;
}
.guide2-wrap .hot-text {
	padding: 0 10px;
}
.guide2-wrap .hot-text h2 {
	font-size: 17px;
	text-align: center;
	white-space: nowrap;
	overflow: hidden;
	margin: 5px 0;
}
.guide2-wrap .hot-text p {
	font-size: 13px;
	line-height: 20px;
	padding-top: 5px;
	margin: 5px 0;
	border-top: 3px solid rgba(255,255,255,.5);
}

/****** 首页引导3******/
.guide3-wrap {
	width: 1024px;
	margin: 0 auto;
}
.guide3-wrap .title {
	color: #2d4e7a;
	font-size: 26px;
	text-align: center;
	padding-top: 45px;
	padding-bottom: 30px;
}
.guide3-wrap .detial-item {
	position: relative;
	width: 250px;
	text-align: center;
	vertical-align: top;
	padding-top: 5px;
	padding-bottom: 59px;
	display: inline-block;
}
.guide3-wrap .detial-item:after {
	content: '';
	position: absolute;
	top: 60px;
	right: -20px;
	width: 24px;
	height: 28px;
	background: url(../img/common_ico.png) no-repeat;
	background-position: -13px -95px;
	display: inline-block;
}
.guide3-wrap .detial-item:last-child:after {
	content: '';
	width: 0;
	height: 0;
}
.guide3-wrap .ico {
	background: url(../img/index_logo_c_s.png) top left no-repeat transparent;
	width: 115px;
	height: 115px;
	display: inline-block;
}
.guide3-wrap .ico.slt-1 {
	background-position: -26px -304px;
}
.guide3-wrap .ico.slt-2 {
	background-position: -26px -444px;
}
.guide3-wrap .ico.slt-3 {
	background-position: -26px -172px;
}
.guide3-wrap .ico.slt-4 {
	background-position: -26px -40px;
}
.guide3-wrap .item-title {
	color: #535353;
	font-size: 20px;
	margin-top: 20px;
	margin-bottom: 10px;
}
.guide3-wrap .item-msg {
	font-size: 14px;
	color: #b3b3b2;
	width: 210px;
	margin: 0 auto;
	line-height: 22px;
}

