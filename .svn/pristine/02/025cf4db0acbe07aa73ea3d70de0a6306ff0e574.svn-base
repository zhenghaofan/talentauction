/* 导航栏 */
.top-bar {
	height: 40px;
	color: #fff;
	font-size: 18px;
	text-align: center;
	line-height: 40px;
	background-color: #2d4e7b;
}
.top-bar .bar-l {
	float: left;
	height: 40px;
}
.top-bar .bar-r {
	float: right;
	height: 40px;
}
.top-bar .bar-ico {
	width: 20px;
	padding: 10px 10px 0;
}
.top-bar .bar-ico.back {
	width: 13px;
	padding-top: 9px;
	padding-left: 16px;
}



/* 导航栏 */
.cv-wrap {
	
}
.cv-wrap .cycle {
	
}
.cv-wrap .cycle .text {
	width: 50%;
	height: 45px;
	font-size: 1.2em;
	text-align: center;
	line-height: 45px;
	border-bottom: 2px solid #F6F6F6;
	display: inline-block;
}
.cv-wrap .cycle .text.l {
	margin-right: -5px;
}
.cv-wrap .cycle .text.l:after {
	content:"";
	float: right;
	width: 1px;
	height: 22px;
	margin-top: 12px;
	margin-right: -0.5px;
	background-color: red;
}
.cv-wrap .cycle .text.slt {
	color: #2d4e7b;
	border-bottom: 2px solid #2d4e7b;
}

.cv-wrap .time {
	height: 60px;
	line-height: 60px;
	text-align: center;
	background-color: #fff;
}
.cv-wrap .time .grid {
	color: #fff;
	padding: 3px 7px;
	border-radius: 2px;
	margin: 0 3px;
	background-color: #808080;
	display: inline;
}


