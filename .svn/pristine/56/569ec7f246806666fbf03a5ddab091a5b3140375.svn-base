/****** 翻页******/
.list-pager {
	height: 45px;
	text-align: center;
	padding-top: 15px;
	border-radius: 0 0 3px 3px;
	background-color: #fff;
}
.list-pager-btn {
	text-align: center;
	margin-right: 10px;
	padding: 5px 10px;
	border: 1px solid #d8dadf;
	border-radius: 3px;
	display: inline-block;
}
.list-pager-btn:hover {
	background-color: #fff;
}
.list-pager a {
	cursor: pointer;
	background-color: #fafafa;
}
.list-pager span {
	padding: 4.5px 10px;
	background-color: #fafafa;
	cursor: pointer;
}
.list-pager .pager-btn-false {
	background-color: #ccc;
	cursor: default;
}
.pager-point {
	margin-right: 10px;
	display: inline-block;
}
.list-pager .slt {
	color: #fff;
	border: 1px solid #4182c6;
	background-color: #4182c6;
}
.list-pager .slt:hover {
	color: #fff;
	background-color: #4182c6;
}


