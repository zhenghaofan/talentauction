

.default-wrap {
	color: #6B7485;
	margin: 130px auto 100px;
	overflow: hidden;
}
.sidebar {
	float: left;
	width: 245px;
	margin-right: 30px;
	border-radius: 5px;
	display: inline-block;
}
.place-box {
	width: 1024px;
	min-height: 900px;
	padding: 15px 30px;
	margin: 0 auto;
	overflow: hidden;
}
.place-box .container {
	width: 747px;
	display: inline-block;
}


/****** 已选择 ******/
.sidebar .seleted-item {
	padding: 20px;
	border-radius: 3px;
	margin-bottom: 20px;
	background-color: #fff;
}
.sidebar .seleted-title {
	font-size: 16px;
	line-height: 24px;
}
.sidebar .seleted-con {
	margin-top: 10px;
}
.sidebar .seleted-con li {
	margin-bottom: 10px;
}
.sidebar .seleted-con-s {
	color: #fff;
	padding: 5px 10px;
	border-radius: 3px;
	background-color: #71a8e0;
	display: inline-block;
}
.sidebar .c-p {
	 cursor: pointer;
}
.sidebar .close_ico {
	 font-style: normal;
	 margin-left: 5px;
}

/* 时间面板*/
.time-wrap {
	border-radius: 3px;
	margin-bottom: 20px;
	background: #70a7df;
}
.time-wrap .time-title {
	color: #fff;
	font-size: 18px;
	padding: 15px 15px;
}
.time-wrap .time-body {
	color: #fff;
	padding: 5px 15px 20px;
}
.time-wrap .time-body span {
	color: #70a7df;
	padding: 2px 6px;
	border-radius: 3px;
	background: #fff;
}

/****** 筛选面板 ******/
.sidebar .regular-list {
	padding: 20px;
	border-radius: 3px;
	background-color: #fff;
}
.sidebar .regular-item {
	margin-bottom: 10px;
}
.sidebar .regular-title {
	font-size: 16px;
}
.sidebar .regular-img {
	width: 16px;
	margin-right: 5px;
	margin-bottom: -1px;
}
.sidebar .img-w12 {
	width: 12px;
}
.sidebar .img-w14 {
	width: 14px;
}
.sidebar .regular-list ul {
	margin-top: 10px;
	padding-bottom: 5px;
}
.sidebar .regular-con li {
	font-size: 15px;
	margin-right: 14px;	
	margin-bottom: 10px;
	display: inline-block;
	cursor: pointer;
}
.sidebar .item-seleted, .regular-con li:hover {
	color: #4182c5;
}

/****** 二维码 ******/
.qrcode-box {
	padding: 20px;
	border-radius: 3px;
	margin: 20px 0;
	background-color: #fff;
}
.qrcode-title {
	font-size: 16px;
	color: #5c6467;
	text-align: center;
}
.qrcode-tip {
	color: #6e7377;
	text-align: center;
}
.qrcode-img {
	width: 200px;
	margin: 0 auto;
	display: inherit;
}

/****** 温馨提示 ******/
.warm-box {
	margin-top: 20px;
	background-color: #fff;
	border-radius: 3px;
}
.warm-title {
	height: 45px;
	color: #4182c6;
	font-size: 16px;
	line-height: 45px;
	padding-left: 15px;
	border-bottom: 1px solid #eceef1;
}
.warm-con {
	line-height: 24px;
	padding: 20px 15px;
}

/****** 加入实力拍 ******/
.hr-qq-box {
	line-height: 30px;
	padding: 20px;
	border-radius: 3px;
	border-top: 4px solid #4182C6;
	margin-top: 20px;
	background-color: #fff;
}
.qq-text {
	font-size: 18px;
}

/****** 专场搜索 ******/
.search-warp {
	margin-bottom: 20px;
}
.search-warp .search-item {
	margin-bottom: 10px;
}
.search-warp .drop-down {
	position: relative;
	width: 106px;
	border-radius: 3px 0 0 3px;	
	background-color: #c3cad5;
	display: inline-block;
	cursor: pointer;
}
.search-warp .drop-btn {
	height: 43px;
	color: #fff;
	line-height: 43px;
	text-align: center;
	display: block;
}
.search-warp ul {
	position: absolute;
	width: 104px;
	border: 1px solid #c3cad5;
	background: #fff;
	display: none;
}
.search-warp ul li {
	height: 35px;
	line-height: 35px;
	text-align: center;
}
.search-warp ul li:hover {
	color: #fff;
	background: #70a7df;
}
.search-warp input {
	width: 480px;
	height: 41px;
	color: #6B7485;
	font-size: 14px;
	text-indent: 12px;
	border-radius: 0;
	margin-left: -5px;
	display: inline-block;
	background-color: #fff;
}
.search-warp .search-btn {
	width: 156px;
	height: 43px;
	color: #fff;
	vertical-align: bottom;
	letter-spacing: 10px;
	border-radius: 0 3px 3px 0;	
	border: none;
	margin-left: -5px;
	background-color: #70a7df;
	cursor: pointer;
}
.search-warp .search-btn:hover {
	background-color: #4182c6;
}

.search-warp .words-item {
	margin-right: 20px;
	cursor: pointer;
}
.search-warp .words-item:hover {
	color: #568cca;
}



/****** 数据列表头******/
.container .header {
	height: 58px;
	padding-left: 30px;
	border-radius: 3px 3px 0 0;
	border-bottom: 1px solid #eceef1;
	background-color: #fff;
}
.header .title-item {
	font-size: 16px;
	padding: 3px 6px;
	border-radius: 3px;
	margin-top: 15px;
	margin-right: 20px;
	display: inline-block;
	cursor: pointer;
}
.header .title-item.f-r {
	float: right;
}
.header .title-item:hover {
	color: #568cca;
}
.header .item-seleted {
	color: #fff !important;
	background: #4182c6;
}
.header .title-slt {
	float: right;
	margin: 17px 25px 0 0;
}



/****** 数据内容******/
.list-box .fc-1 {
	color: #535A62;
}
.list-box .fz-1 {
	font-size: 14px;
}
.list-box .bid-item {
	height: 80px;
	color: #8b93a0;
	outline: white;
	padding: 15px 0;
	border-bottom: 1px solid #eceef1;
	background-color: #fff;
	display: block;
}
.bid-item:hover {
	background-color: #E6E8EC;
}
.title-l, .info-l {
	width: 48%;
	font-size: 16px;
	margin: 0px 5px 10px 35px;
	display: inline-block;
}
.title-r, .info-r {
	width: 40%;
	margin: 0px 35px 10px 5px;
	display: inline-block;
}
.title-r span {
	max-width: 140px;
	font-size: 12px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	padding: 4px 8px;
	border: 1px solid #8d96a5;
	margin-right: 10px;
}
.info-l, .info-r {
	font-size: 14px;
	line-height: 26px;
}




.list-box  .list-con-filling {
	height: 150px;
	color: #616268;
	padding-top: 100px;
	text-align: center;
	background-color: #fff;
}

/****** 页尾******/
#footer {
	position: relative;
}

