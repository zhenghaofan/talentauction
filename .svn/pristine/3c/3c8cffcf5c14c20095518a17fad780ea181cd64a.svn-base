

.default-wrap {
	color: #6B7485;
	margin: 100px auto 80px;
	overflow: hidden;
}
.sidebar {
	float: left;
	width: 245px;
	margin-right: 30px;
	border-radius: 5px;
	display: inline-block;
}
.place-box {
	width: 1024px;
	min-height: 900px;
	/* padding: 15px 30px; */
	padding-bottom: 2px;
	margin: 0 auto;
	overflow: hidden;
}
.place-box .container {
	width: 747px;
	display: inline-block;
}
.title-ico, .opt-ico-min {
	vertical-align: middle;
	background: url("../img/bg-ico.png") no-repeat;
	display: inline-block;
}
.opt-ico-min.up-ico {
	width: 16px;
	height: 20px;
	background-position: -4px -168px;
}
.opt-ico-min.del-ico {
	width: 17px;
	height: 20px;
	background-position: -59px -169px;
}

/* 提示标签*/
.ring-tag {
	max-width: 140px;
	color: #71a8e0;
	font-size: 12px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	padding: 2.5px 6px;
	border: 1px solid #71a8e0;
	margin-right: 10px;
}
.push-tag {
	width: 70px;
	height: 20px;
	color: #fff;
	font-size: 12px;
	text-align: center;
	line-height: 20px;
	vertical-align: 3px;
	border-radius: 5px 5px 5px 0;
	background-color: #71a8e0;
	display: inline-block;
}


/****** 已选择 ******/
.sidebar .seleted-item {
	padding: 20px;
	border-radius: 3px;
	margin-bottom: 20px;
	background-color: #fff;
}
.sidebar .seleted-title {
	font-size: 16px;
	line-height: 24px;
}
.sidebar .seleted-con {
	margin-top: 10px;
}
.sidebar .seleted-con li {
	margin-bottom: 10px;
}
.sidebar .seleted-con-s {
	color: #fff;
	padding: 5px 10px;
	border-radius: 3px;
	background-color: #71a8e0;
	display: inline-block;
}
.sidebar .c-p {
	 cursor: pointer;
}
.sidebar .close_ico {
	 font-style: normal;
	 margin-left: 5px;
}

/* 需求面板*/
.jd-wrap {
	color: #fff;
	font-size: 16px;
	padding: 10px 20px;
	border-radius: 3px;
	margin-bottom: 20px;
	background: url("../img/bg-ico.png") no-repeat;
    background-position: -3px -436px;
	cursor: pointer;	
}
.jd-wrap:hover {
    background-position: -3px -495px;
}
.jd-wrap img {
	vertical-align: -1px;
}
.jd-wrap .edit-ico {
	width: 18px;
	height: 34px;
	background-position: -124px -52px;
}

/* 时间面板*/
.time-wrap {
	border-radius: 3px;
	margin-bottom: 20px;
	background: #fff;
}
.time-wrap .time-title {
	font-size: 16px;
	padding: 12px 20px;
}
.time-wrap .time-body {
	padding: 5px 20px 15px;
}
.time-wrap .time-body span {
	color: #fff;
	padding: 2px 5px;
	border-radius: 3px;
	background: #c3cad5;
}

/****** 筛选面板 ******/
.sidebar .regular-list {
	padding: 20px;
	border-radius: 3px;
	background-color: #fff;
}
.sidebar .ico-1 {
	width: 18px;
	height: 34px;
	background-position: -3px -55px;	
}
.sidebar .ico-2 {
	width: 24px;
	height: 34px;
	background-position: -229px -27px;
}
.sidebar .ico-3 {
	width: 13px;
	height: 34px;
	background-position: -30px -55px;
}
.sidebar .ico-4 {
	width: 22px;
	height: 34px;
	background-position: -169px -26px;
}

.sidebar .regular-item {
	margin-bottom: 10px;
}
.sidebar .regular-title {
	font-size: 16px;
}
.sidebar .regular-img {
	width: 16px;
	margin-right: 5px;
	margin-bottom: -1px;
}
.sidebar .img-w12 {
	width: 12px;
}
.sidebar .img-w14 {
	width: 14px;
}
.sidebar .regular-list ul {
	margin-top: 10px;
	padding-bottom: 5px;
}
.sidebar .regular-con li {
	font-size: 15px;
	margin-right: 14px;	
	margin-bottom: 10px;
	display: inline-block;
	cursor: pointer;
}
#skill ul li {
	display: block;
}
.sidebar .item-seleted, .regular-con li:hover {
	color: #71a8e0;
}

/****** 二维码 ******/
.qrcode-box {
	padding: 20px;
	border-radius: 3px;
	margin: 20px 0;
	background-color: #fff;
}
.qrcode-title {
	font-size: 16px;
	color: #5c6467;
	text-align: center;
}
.qrcode-tip {
	color: #6e7377;
	text-align: center;
}
.qrcode-img {
	width: 200px;
	margin: 0 auto;
	display: inherit;
}

/****** 温馨提示 ******/
.warm-box {
	margin-top: 20px;
	background-color: #fff;
	border-radius: 3px;
}
.warm-title {
	height: 45px;
	color: #4182c6;
	font-size: 16px;
	line-height: 45px;
	padding-left: 15px;
	border-bottom: 1px solid #eceef1;
}
.warm-con {
	line-height: 24px;
	padding: 20px 15px;
}
.warm-con ul {
	list-style-type:square; 
	padding:0px 16px;	
}

/****** 加入实力拍 ******/
.hr-qq-box {
	line-height: 30px;
	padding: 20px;
	border-radius: 3px;
	border-top: 4px solid #4182C6;
	margin-top: 20px;
	background-color: #fff;
}
.qq-text {
	font-size: 18px;
}

/****** 专场搜索 ******/
.search-warp {
	margin-bottom: 20px;
}
.search-warp .search-item {
	margin-bottom: 10px;
}
.search-warp .search-item.inp-box {
	border-radius: 3px;
	border: 2px solid #70a7df;
	overflow: hidden;
}
.search-warp input {
	width: 643px;
	height: 38px;
	color: #6B7485;
	font-size: 14px;
	text-indent: 12px;
	border-radius: 0;
	border: 0;
	display: inline-block;
	background-color: #fff;
}
.search-warp .search-btn {
	float: right;
	width: 100px;
	height: 38px;
	color: #fff;
	font-size: 18px;
	vertical-align: bottom;
	/* letter-spacing: 10px; */
	border: 0;
	background-color: #70a7df;
	cursor: pointer;
}
.search-warp .search-btn:hover {
	background-color: #4182c6;
}

.search-warp .words-item {
	margin-right: 20px;
	cursor: pointer;
}
.search-warp .words-item:hover {
	color: #568cca;
}


/****** 数据列表头******/
.container .header {
	height: 58px;
	padding-left: 30px;
	/* border-radius: 3px 3px 0 0; */
	border-radius: 0;
	border-bottom: 1px solid #eceef1;
	background-color: #fff;
}
.header .title-item {
	font-size: 16px;
	padding: 3px 6px;
	border-radius: 3px;
	margin-top: 15px;
	margin-right: 20px;
	display: inline-block;
	cursor: pointer;
}
.header .title-item.f-r {
	float: right;
}
.header .title-item:hover {
	color: #71a8e0;
}
.header .item-seleted {
	color: #71a8e0 !important;
	/* background: #71a8e0; */
}
/* .header .title-slt {
	float: right;
	margin: 17px 25px 0 0;
} */



/****** 数据内容******/
.list-box .fc-1 {
	color: #535A62;
}
.list-box .fc-2 {
	color: #4182c6;
}
.list-box .fc-3 {
	color: #71a8e0;
}
.list-box .fz-1 {
	font-size: 14px;
}
.list-box .fz-2 {
	font-size: 20px;
}
.list-box .bid-item {
	position: relative;
	height: 80px;
	color: #8b93a0;
	outline: white;
	padding: 15px 0;
	border-bottom: 1px solid #eceef1;
	background-color: #fff;
	display: block;
}
.list-box .bid-item .look {
	position: absolute;
	left: -9px;
	width: 46px;
	height: 32px;
	color: #fff;
	line-height: 26px;
	text-indent: 5px;
	background: url("../img/ico_haslook.png");
	display: inline-block;
}
.list-box .bid-item .bid {
	position: absolute;
	left: -9px;
	width: 46px;
	height: 32px;
	color: #fff;
	line-height: 26px;
	text-indent: 5px;
	background: url("../img/ico_hasbid.png");
	display: inline-block;	
}
.bid-item:hover {
	background-color: #E6E8EC;
}
.title-l, .info-l {
	width: 46%;
	font-size: 16px;
	margin: 0px 5px 10px 45px;
	display: inline-block;
}
.title-l .ellipsis {
	min-width: 50px;
	max-width: 205px;
	text-overflow: ellipsis;
	white-space: nowrap;
	vertical-align: middle;
	overflow: hidden;
	display: inline-block;
}
.title-r, .info-r {
	width: 40%;
	margin: 0px 35px 10px 5px;
	display: inline-block;
}
.info-l, .info-r {
	font-size: 14px;
	line-height: 26px;
}
.info-r .suit-num {
	float: right;
	margin-top: -34px;
}
.info-r .suit-num .fc-2 {
	vertical-align: middle;
}

/****** 暂无数据******/
.list-box  .list-con-filling {
	height: 150px;
	color: #616268;
	padding-top: 100px;
	text-align: center;
	background-color: #fff;
}


/****** 引导页******/
.guide-modal {
	display: none;
}
.guide-modal .fc-1 {
	color: #70a7df;
	vertical-align: middle;
}
.guide-modal .anime-wrap {
	width: 680px;
	height: 155px;
	position: relative;
	margin: 80px auto 0;
}
.guide-modal .modal-dialog {
	width: 750px;
	margin-top: 0;
}
.guide-modal .modal-content {
	color: #6B7485;
	border-radius: 2px;
}
.guide-modal .modal-header {
	padding-right: 25px;
}
.guide-modal .modal-title span {
	font-size: 18px;
	margin-left: 16px;
}
.guide-modal .close {
	font-weight: normal;
	background-color: #fff;
}
.guide-modal .modal-body {
	padding: 0 15px;
	overflow: hidden;
}
.guide-modal .jd-l {
	float: left;
	width: 358px;
	height: 370px;
}
.guide-modal .jd-r {
	float: left;
	width: 358px;
	height: 370px;
	border-left: 1px solid #e5e5e5;
}
.guide-modal .jd-list {
	height: 285px;
	margin: 25px 25px 0 40px;
}
.guide-modal .jd-list li {
	height: 32px;
	font-size: 13px;
	line-height: 32px;
	border-bottom: 1px solid #e5e5e5;
	cursor: pointer;
}
.guide-modal .jd-list li a {
	color: #6B7485;
}
.guide-modal .jd-list li .link:hover {
	color: #71A8E0;
}
.guide-modal .opt-ico {
	float: right;
	/* height: 20px; */
	/* margin-top: -1px; */
	display: none; 
	cursor: pointer;
}
.guide-modal .opt-ico img {
	width: 16px;
}
.guide-modal .btn {
	color: #fff;
	border: none;
	border-radius: 3px;
	background-color: #71A8E0;
}
.guide-modal .save {
	width: 90px;
	height: 38px;
}
.guide-modal .dis {
	color: #6B7485;
	width: 90px;
	height: 38px;
	background-color: #ECEDF1;
}
.guide-modal .save:hover {
	color: #fff;
	background-color: #4182c6;
}
.guide-modal input {
	width: 210px;
	height: 36px;
	color: #6B7485;
	font-size: 14px;
	text-indent: 8px;
	border: 1px solid #F3F1F1;
	display: inline-block;
	background-color: #eceef1;
	border-radius: 3px;
}
.guide-modal input.small {
	width: 40px;
}
.guide-modal textarea {
	width: 194px;
	height: 64px;
	color: #6B7485;
	padding: 5px 8px;
	margin-left: 2px;
	resize: none;
	background-color: #ECEDF1;
}
.guide-modal .inp-item:first-child {
	margin-top: 20px;
}
.guide-modal .inp-item:last-child {
	text-align: center;
}
.guide-modal .remarks {
	color: #6B7485;
	font-size: 14px;
	margin: 0 6px 0 28px;
}
.guide-modal .err-wrap {
	min-height: 25px;
}
.guide-modal .err-wrap label {
	height: 25px;
	color: #FF1414;
	line-height: 25px;
	text-indent: 20px;
	padding-left: 18px;
	margin-left: 85px;
	display: none;
}
.guide-modal .err-wrap .ico-invalid {
	background: url(../img/icon-error.png) 0 45% no-repeat;
}



/****** 页尾******/
#footer {
	position: relative;
}

