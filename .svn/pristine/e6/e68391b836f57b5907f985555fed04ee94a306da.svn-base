

.fc-1 {
	color: #4182c4;
}
.fc-2 {
	color: #e4785b;
}
.fc-3 {
	color: #71a8e0;
}
.fz-1 {
	font-size: 13px;
}
.fb {
	font-weight: bold;
}
pre {
	color: #6B7485;
	font-size: 14px;
}

.bid-wrap {
	width: 1024px;
	color: #6B7485;
	padding-bottom: 2px;
	margin: 100px auto 80px; 
	overflow: hidden;
}
.bid-wrap .title-ico {
	vertical-align: middle;
	background: url("../img/bg-ico.png") no-repeat;
	display: inline-block;
}
.tipso-bubble {
	position: absolute;
	top: 30px;
	left: 50%;
	color: #fff;
	min-width: 80px;
	text-align: center;
	padding: 8px;
	border-radius: 6px;
	margin-left: 140px;
	z-index: 9999;
	background: rgb(113, 168, 224);
	display: none;
}
.tipso-arrow {
	position: absolute;
	top: 50%;
	left: -16px;
	margin-top: -8px;
	border: 8px solid transparent;
	border-right-color:rgb(113, 168, 224);
	pointer-events: none;
}


/* 左侧栏*/
.bid-box-l {
	float: left;
	width: 700px;
	border-radius: 3px;
	background-color: #fff;
	display: inline-block;
}
.info-item {
	border-bottom: 1px dotted #e5e5e5;
	margin-bottom: 20px;	
}
.info-item:last-child {
	border-bottom: 0;
}
.info-item ul {
	margin-bottom: 30px;
}
.info-item.edu ul {
	margin-bottom: 0;
}
.info-item ul:last-child {
	margin-bottom: 0;
}
.info-item .item-title {
	height: 30px;
	font-size: 18px;
	line-height: 30px;
	padding-left: 40px;
}
.info-item .title-ico.ico-1 {
	width: 23px;
	height: 34px;
	background-position: -169px -25px;
}
.info-item .title-ico.ico-2 {
	width: 21px;
	height: 34px;
	background-position: -201px -26px;
}
.info-item .title-ico.ico-3 {
	width: 19px;
	height: 34px;
	background-position: -233px -26px;
}
.info-item .title-ico.ico-4 {
	width: 18px;
	height: 34px;
	background-position: -264px -25px;
}
.info-item .title-ico.ico-5 {
	width: 16px;
	height: 34px;
	background-position: -348px -25px;
}
.info-item .title-ico.ico-6 {
	width: 14px;
	height: 34px;
	background-position: -374px -25px;
}





.info-item .item-img {
	width: 20px;
	margin-bottom: -1px;
}
.info-item .item-img.edu {
	width: 24px;
}
.info-item .item-con {
	font-size: 14px;
	line-height: 30px;
	margin: 15px 40px 0;
	padding-bottom: 20px;
}
.info-item .item-con a {
	color: #6B7485;
	word-break: break-all;
}
.info-item .item-con a:hover {
	color: #3fa9f5;
}
.info-item .resume-box {
	padding: 10px;
	max-height: 400px;
	text-align: center;
}
.info-item .resume-box .resume-img {
	max-width: 600px;
	max-height: 400px;
}

/* 基本信息*/
.base-info .bid-name {
	color: #71a8e0;
	font-size: 20px;
	margin-bottom: 10px;
}
.base-info .bid-state {
	font-size: 16px;
}
.base-info .line {
	margin: 0 2px 0 10px;
}
.base-info .push-tag {
	width: 70px;
	height: 20px;
	color: #fff;
	font-size: 12px;
	text-align: center;
	line-height: 20px;
	vertical-align: 3px;
	border-radius: 5px 5px 5px 0;
	background-color: #71a8e0;
	display: inline-block;
}
.base-info .code {
	float: right;
	font-size: 14px;
	color: #8b93a0;
}
.base-info .ring-tag {
	max-width: 140px;
	color: #4182c6;
	font-size: 12px;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	padding: 2.5px 6px;
	border: 1px solid #4182c6;
	margin-right: 10px;
}


/* 右侧栏*/
.bid-con-r {
	float: right;
	width: 300px;
	padding-right: 2px;
}

/* 竞拍*/
.bid-box {
	color: #fff;
	padding: 30px 30px 15px;
	border-radius: 3px;
	margin-bottom: 22px;
	background-color: #2d4e7b;
}
.bid-box .title {
	font-size: 18px;
	margin-bottom: 20px;
}
.bid-box .inp-box {
	margin-bottom: 10px;
}
.bid-box .inp-box input {
	height: 35px;
	width: 50px;
	font-size: 16px;
	text-indent: 6px;
	border-radius: 3px;
}
.bid-box .inp-box .bid-unit {
	vertical-align: bottom;
	margin-left: 5px;
	display: inline-block;
}
.bid-box .ccl-ico {
	font-size: 20px;
	vertical-align: middle;
	margin: 0 10px 0px 6px;
}
.bid-box .check-box {
	height: 40px;
	font-size: 16px;
	text-align: left;
	line-height: 32px;
}
.bid-box .check-box input {
	margin-right: 5px;
}
.bid-box .jobs-label {
	display: block;
	margin-bottom: 10px;
	font-size: 16px;
}
.bid-box .job-text {
	width: 92%;
	height: 100px;
	padding: 10px;
	resize: none;
	line-height: 20px;
}
.bid-box .btn-con {
	text-align: center;
	padding: 15px 0;
}
.bid-box .bid-btn {
	width: 233px;
	color: #fff;
	font-size: 20px;
	padding: 14px 0;
	border-radius: 3px;
	background: url("../img/bg-ico.png") no-repeat;
    background-position: -6px -559px;
	display: inline-block;
	cursor: pointer;
}
.bid-box .bid-btn:hover {
	background: url("../img/bg-ico.png") no-repeat;
    background-position: -5px -617px;
}
.bid-box .err-wrap {
	min-height: 25px;
}
.bid-box .err-wrap label {
	height: 25px;
	color: #FF1414;
	line-height: 25px;
	text-indent: 20px;
	padding-left: 18px;
	display: none;
}
.bid-box .err-wrap .ico-invalid {
	background: url(../img/icon-error.png) 0 45% no-repeat;
}


/* 温馨提示*/
.warm-box {
	color: #656769;
	margin-top: 20px;
	background-color: #fff;
	border-radius: 3px;
}
.warm-title {
	height: 45px;
	color: #4182c5;
	font-size: 16px;
	line-height: 45px;
	padding-left: 30px;
	border-bottom: 1px solid #eceef1;
}
.warm-con {
	padding: 20px 15px;
	line-height: 24px;
}
.warm-item {
	text-indent: -15px;
	padding-left: 15px;
}


/****** 页尾******/
#footer {
	position: relative;
}