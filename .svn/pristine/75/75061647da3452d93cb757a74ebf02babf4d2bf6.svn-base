.slt-wrap {
	width: 720px;
	color: #6B7485;
	margin: 130px auto 100px;
	border-radius: 3px;
	background-color: #fff;
}


.nav-warp {
	text-align: center;
	padding-top: 30px;
}
.nav-warp .tab-item {
	width: 92px;
	text-align: center;
	vertical-align: top;
	margin-left: -34px;
	margin-top: 3px;
	display: inline-block;
}
.nav-warp .line {
	width: 120px;
	height: 7px;
	vertical-align: top;
	margin-top: 10px;
	margin-left: -32px;
	background-color: #d8dbe0;
	display: inline-block;
}
.nav-warp .tab-step {
	width: 20px;
	height: 20px;
	color: #fff;
	font-size: 14px;
	text-align: center;
	line-height: 20px;
	border-radius: 25px;
	border: 1px solid #d8dbe0;
	margin-bottom: 5px;
	background-color: #d8dbe0;
	display: inline-block;
}
.nav-warp .tab-text {
	color: #d8dbe0;
}
.nav-warp .slt {
	color: #6B7485;
	font-size: 15px;
}
.nav-warp .slt .tab-step {
	width: 23px;
	height: 23px;
	font-size: 15px;
	line-height: 23px;
	margin-bottom: 8px;
	border-radius: 15px;
	border: 1px solid #71A8E0;
	background-color: #71A8E0;
}
.nav-warp .slt .tab-text {
	color: #6B7485;
	font-size: 15px;
}
