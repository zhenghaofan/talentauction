
body {
	background-color: #E2E2E2;
	-webkit-overflow-scrolling: touch;
}
.fc-1 {
	color: #3a8acf;
}
.fc-2 {
	color: #8c8c8c;
}
.m-r10 {
	margin-right: 10px;
}
.m-t5 {
	margin-top: 5px;
}
.m-t6 {
	margin-top: 7px;
}
.m-b5 {
	margin-bottom: 5px;
}


/* 导航栏 */
.top-bar {
	height: 40px;
	color: #fff;
	font-size: 18px;
	line-height: 40px;
	text-align: center;
	background-color: #2d4e7b;
}
.top-bar .top-bar-spec {
	float: right;
	height: 40px;
}
.top-bar .top-bar-query {
	float: left;
	height: 40px;
}
.top-bar .top-bar-ico {
	width: 20px;
	padding: 10px 10px 0;
}
.top-bar .top-bar-ico.back {
	width: 13px;
	padding-left: 16px;
	padding-top: 9px;
}


/* 主页面 */
.body2 {
	z-index: 10;
	position: relative;
	height: 100%;
	left: 0;
	-webkit-transition: .2s ease left;
	transition: .2s ease left;
}
/* 拍卖周期切换*/
.cycle {
	background-color: #f6f6f6;
}
.cycle .text {
	width: 50%;
	height: 45px;
	font-size: 1.2em;
	text-align: center;
	line-height: 45px;
	border-bottom: 2px solid #F6F6F6;
	display: inline-block;
}
.cycle .text.l {
	margin-right: -5px;
}
.cycle .text.l:after {
	content:"";
	float: right;
	width: 1px;
	height: 26px;
	margin-top: 10px;
	margin-right: -0.5px;
	background-color: #dbdbdb;
}
.cycle .text.slt {
	color: #5b8dc8;
	border-bottom: 2px solid #5b8dc8;
}
/* 拍卖时间*/
.spec-detial-title {
	width: 100%;
	height: 44px;
	font-size: 16px;
	text-align: center;
	line-height: 44px;
	border-bottom: 1px solid #dbdbdb;
	background-color: #fff;
}
.spec-det-time {
	margin-right: 5px;
}
.spec-det-clock {
	color: #fff;
	padding: 3px 6px;
	border-radius: 2px;
	margin: 0 3px;
	display: inline;
	background-color: #808080;
}
/* 数据列表*/
.spec-detial-list {
	overflow: scroll;
	background-color: #fff;
	-webkit-transform: translate3d(0,0,0);
}
.spec-det-box {
	 width: 90%;
	 color: #333;
	 padding: 8px 5%;
	 border-bottom: 1px solid #dbdbdb;
	 display: block;
}
.spec-item {
	width: 100%;
}
.spec-title {
	font-size: 1.2em;
	max-width: 200px;;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	display: inline-block;
}
.spec-no {
	display: inline-block;
}
.spec-info {
	float: left;
	width: 60%;
}
.spec-introduct {
	float: right;
	text-align: center;
}
.spec-intro-item {
	min-width: 70px;
	max-width: 120px;
	color: #727272;
	padding: 2px 0;
	border: 1px solid #727272;
}




/* 筛选栏 */
.sidebar-r {
	position: absolute;
	z-index: 1;
	width: 219px;
	height: 100%;
	top: 40px;
	right: 0px;
	color: #0f0f0f;
	border-left: 1px solid #ccc;
	background-color: #f9f9f9;
	overflow: scroll;
	display: none;
}
.side-r-con {
	overflow-x: scroll;
	overflow: auto;
}
.side-r-item {
	border-top: 1px solid #E0E0E0;
}
.side-r-item-title {
	font-size: 16px;
	line-height: 40px;
	padding: 0 20px;
}
.query-s-c {
	color: #4181c8;
}
.side-r-item-ico-d {
	float: right;
	width: 18px;
	margin-top: 10px;
}
.side-r-item-ul {
	background-color: #fff;
	display: none;
}
.side-r-item-li {
	line-height: 35px;
	padding: 0 20px;
	border-top: 1px solid #c1c1c1;
}
.side-r-item-s {
	color: #4181c8;
}
.side-r-item-ul .b-t0 {
	border-top: 0;
}
.side-r-item-s-ico {
	float: right;
	width: 18px;
	margin-top: 10px;
	display: none;
}
.side-r-item-s .side-r-item-s-ico {
	display: inline-block;
}
.b-b-e0 {
	border-bottom: 1px solid #E0E0E0;
}
.main-bg {
	z-index: 10000;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}







