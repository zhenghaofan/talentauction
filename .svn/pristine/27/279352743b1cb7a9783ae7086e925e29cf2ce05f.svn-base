.logo {
	display: inline-block;
	height: 55%;
}

.navbar-brand>img {
  margin: 11px auto 0!important;
}

.bs-docs-nav .navbar-toggle .icon-bar{
	background-color: #2d4e7b;
}

.bs-docs-nav {
	margin-bottom: 0;
	background-color: #fff;
	border-bottom: 0;
}

.bs-docs-nav .navbar-header .navbar-toggle {
	border-color: #fff;
}

.intro-block{
	width: 100%;
}

.intro-block img{
	width: 100%;
}

.step-block{
	margin-top: 3em;
}

.step-block img{
	width: 90%;
}

.comp-block{
	margin-top: 5em;
}

.comp-block img{
	width: 90%;
}

.container-fluid{
	text-align: center;
}

h4{
	margin-bottom: 2em;
}


.go-block{
	margin-top: 5em;
}

.go-block img{
	width: 60%;
	position: relative;
	float: left;
}

.go-block h5{
	display: inline-block;
	position: relative;
	float: right;
	width: 40%;
	text-align: left;
	top: 2.4em;
	font-size: 1.2em;
}
