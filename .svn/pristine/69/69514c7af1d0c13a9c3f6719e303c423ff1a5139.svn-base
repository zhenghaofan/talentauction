}


/****** 企业引导1******/
.guide1-wrap {
	height: 400px;
	margin-top: 46px;
	overflow: hidden;
	background: url(../img/comp_banner.jpg) 48% 45% no-repeat;
}
.guide1-wrap .text-1 {
	color: #2d4e7a;
	font-size: 30px;
	text-align: center;
	margin-top: 120px;
	margin-bottom: 20px;
}
.guide1-wrap .text-2 {
	color: #2d4e7a;
	font-size: 18px;
	text-align: center;
}
.guide1-wrap .text-3 {
	position: absolute;
	top: 285px;
	left: 50%;
	text-align: center;
	margin-left: -66px;
}
.guide1-wrap .text-3 a {
	height: 40px;
	width: 120px;
	color: #fff;
	font-size: 18px;
	line-height: 40px;
	background: #48bde7;
	display: inline-block;
}
.guide1-wrap .text-3 a:hover{
	background-color: #4182c6;
}

/****** 企业引导2******/
.guide2-wrap {
	width: 1024px;
	margin: 0 auto;
}
.guide2-wrap .title {
	color: #2d4e7a;
	font-size: 26px;
	text-align: center;
	padding-top: 45px;
	padding-bottom: 30px;
}
.guide2-wrap .detial-item {
	position: relative;
	width: 250px;
	text-align: center;
	vertical-align: top;
	padding-top: 5px;
	padding-bottom: 59px;
	display: inline-block;
}
.guide2-wrap .detial-item:after {
	content: '';
	position: absolute;
	top: 60px;
	right: -20px;
	width: 24px;
	height: 28px;
	background: url(../img/common_ico.png) no-repeat;
	background-position: -13px -95px;
	display: inline-block;
}
.guide2-wrap .detial-item:last-child:after {
	content: '';
	width: 0;
	height: 0;
}
.guide2-wrap .ico {
	background: url(../img/index_logo_c_s.png) top left no-repeat transparent;
	width: 115px;
	height: 115px;
	display: inline-block;
}
.guide2-wrap .ico.slt-1 {
	background-position: -26px -1130px;
}
.guide2-wrap .ico.slt-2 {
	background-position: -26px -1245px;
}
.guide2-wrap .ico.slt-3 {
	background-position: -26px -1372px;
}
.guide2-wrap .ico.slt-4 {
	background-position: -26px -1496px;
}
.guide2-wrap .item-title {
	color: #535353;
	font-size: 20px;
	margin-top: 20px;
	margin-bottom: 10px;
}
.guide2-wrap .item-msg {
	font-size: 14px;
	color: #b3b3b2;
	width: 195px;
	margin: 0 auto;
	line-height: 22px;
}

