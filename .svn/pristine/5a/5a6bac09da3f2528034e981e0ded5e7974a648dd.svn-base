


.f-c-1 {
  color: #5388ca;
}


.info-warp {
	width: 1024px;
	margin: 130px auto 100px;
	border-radius: 3px;
	color: #6B7485;	
}

.info-title {
	text-align: center;
	padding-top: 35px;
	border-radius: 3px;
	background-color: #fff;
}
.info-title .tab-item {
	width: 60px;
	text-align: center;
	vertical-align: top;
	margin-left: -20px;
	margin-top: 3px;
	display: inline-block;
}
.info-title .tab-item:first-child {
	margin-left: 0;
}
.info-title .tab-item-step {
	color: #fff;
	width: 20px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	font-size: 14px;
	margin-bottom: 5px;
	border-radius: 22px;
	border: 1px solid #d8dbe0;
	background-color: #d8dbe0;	
	display: inline-block;
}
.info-title .tab-item-text {
	color: #d8dbe0;
}
.info-title .tab-item-slt {
	width: 64px;
	margin-top: 0;
}
.info-title .tab-item-slt .tab-item-step {
	width: 25px;
	height: 25px;
	color: #fff;
	font-size: 15px;
	line-height: 25px;
	text-align: center;
	margin-bottom: 10px;
	border-radius: 15px;
	border: 1px solid #70a7e0;
	background-color: #70a7e0;
	display: inline-block;
}
.info-title .tab-item-slt .tab-item-text {
	color: #6B7485;
	font-size: 15px;
}
.info-title .tab-line {
	width: 115px;
	height: 7px;
	vertical-align: top;
	margin-top: 10px;
	margin-left: -20px;
	background-color: #d8dbe0;
	display: inline-block;
}

.info-title .tip-wrap {
	height: 60px;
	line-height: 60px;
	text-align: center;
	background-position: 50px 50%;
}
.info-title .tip-wrap label {
	color: #4884c2;
}
.info-title .tip-wrap .ico {
	width: 14px;
	height: 14px;
	vertical-align: middle;
	margin-top: -4px;
	background: url(/img/icon-error-grey.png) top left no-repeat transparent;
	display: inline-block;
}
.info-title .slt-wrap .btn {
	width: 140px;
	height: 45px;
	color: #fff;
	line-height: 45px;
	border-radius: 4px;
	background-color: #71a8e0;
	display: inline-block;
	cursor: pointer;
}
.info-title .slt-wrap .btn.slt {
	background-color: #4182c6;
	cursor: auto;
}
.info-title .btn .rand {
	width: 17px;
	height: 17px;
	vertical-align: middle;
	margin-top: -4px;
	border-radius: 8px;
	background-color: #fff;
	display: inline-block;
}
.info-title .btn .rand.slt {
	background: url(/img/w_gou.png) center center no-repeat #fff;
}


.body-wrap {
	margin: 30px 0 100px;
	background-color: #fff;
}
.pre-btn {
  width: 115px;
  height: 45px;
  color: #fff;
  line-height: 45px;
  text-align: center;
  margin-right: 20px;
  border-radius: 5px;
  background-color: #dadbe0;
  display: inline-block;
  cursor: pointer;
}
.next-btn {
  width: 115px;
  height: 45px;
  color: #fff;
  line-height: 45px;
  text-align: center;
  border-radius: 5px;
  background-color: #75a8e1;
  display: inline-block;
  cursor: pointer;
}

.btn {
  color: #fff;
  text-align: center;
  background-color: #70a7df;
  cursor: pointer;
}
.sub-btn {
	height: 44px;
	width: 150px;
	font-size: 14px;
	border: none;
	border-radius: 5px;
	margin-right: 20px;
	background-color: #73a6e5;
	color: #fff;
}
.submit {
	height: 44px;
	width: 115px;
	font-size: 14px;
	border: none;
	border-radius: 5px;
}
.btn:hover {
	background-color: #4182c6;
}
.pre-btn:hover {
	background-color: #ced1d6;
}
.next-btn:hover {
	background-color: #4182c6;
}
/****** 左侧栏******/
.sidebar {
	float: left;
	width: 270px;
	position: relative;
}
.sidebar .cv-pro {
	padding: 30px 25px;
	margin-bottom: 30px;
	background-color: #fff;
	border-radius: 3px;
}
.sidebar .pro-title {
	font-size: 16px;
	margin-bottom: 15px;
}
.sidebar .pro-text {
	margin-bottom: 20px;
}
.sidebar .line {
	height: 5px;
	border-radius: 2px;
	background-color: #edeef2;
}
.sidebar .slider {
	width: 0;
	height: 5px;
	background-color: #4182c4;
	border-radius: 2px;
}
.sidebar .cv-ico {
  padding-left: 25px;
}
.sidebar .ico-line {
  width: 1px;
  background-color: #b9bcc1;
  float: left;
  height: 170px;
}
.sidebar .ico-line-l {
	height: 257px;
}
.sidebar .ico-item {
  margin-top: 25px;
  cursor: pointer;
}
.sidebar .ico-item.first {
	padding-top: 8px;
}
.sidebar .ico-point {
	width: 14px;
	height: 14px;
	line-height: 12px;
	text-align: center; 
	border-radius: 7px;
	margin-left: -7px;
	margin-right: 15px;
	background-color: #f4f4f4;
	display: inline-block;
}
.sidebar .ico-point-p {
	width: 8px;
	height: 8px;
	border-radius: 5px;
	background-color: #b8bbc0;
	display: inline-block;
}
.sidebar .ico-person {
	width: 20px;
	margin-right: 20px;
	display: inline-block;
}
.sidebar .ico-text {
	display: inline-block;
}
.sidebar .ico-base {
	width: 22px;
	height: 20px;
	vertical-align: middle;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -34px -235px;
	display: inline-block;
}
.sidebar .ico-edu {
	width: 22px;
	height: 20px;
	vertical-align: middle;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -32px -257px;
	display: inline-block;
}
.sidebar .ico-exp {
	width: 22px;
	height: 18px;
	vertical-align: middle;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -32px -280px;
	display: inline-block;
}
.sidebar .ico-pro {
	width: 22px;
	height: 20px;
	vertical-align: middle;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -32px -296px;
	display: inline-block;
}
/****** 右侧栏******/
.rightbar {
	float: right;
	width: 727px;
	margin-left: 23px;
	margin-bottom: 100px;
	background-color: #fff;	
}
.rightbar .cv-title {
	width: 727px;
	height: 133px;
	background: url(/img/online_resume_bg.jpg) top left no-repeat transparent;
}

.rightbar .edit-ico {
	float: right;
	width: 28px;
	height: 28px;
	margin-right: 20px;
	margin-top: 10px;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -31px -318px;
	cursor: pointer;
}
.rightbar .edit-box {
	text-align: center;
	margin-bottom: 10px;
}
.rightbar .edit-btn {
	width: 260px;
	height: 45px;
	color: #fff;
	font-size: 16px;
	line-height: 45px;
	border: 1px dashed #fff;
	display: inline-block;
	cursor: pointer;
}
.rightbar .base-msg {
	color: #fff;
	text-align: center;
}
.rightbar .base-sex, .base-age, .base-phone {
	margin-right: 25px;
	display: inline-block;
}
.rightbar .base-edu, .base-email {
	display: inline-block;
}
.rightbar .base-phone i {
	width: 20px;
	height: 20px;
	margin-bottom: -2px;
	margin-right: 5px;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -31px -15px;
	display: inline-block;
}
.rightbar .base-email i {
	width: 20px;
	height: 14px;
	margin-right: 5px;
	background: url(/img/online_resume_ico_c.png) no-repeat transparent;
	background-position: -37px -105px;
	display: inline-block;
}
.rightbar .resume-item {
	color: #4a4f52;
	padding: 25px 30px;
	border-bottom: 1px dashed #edeef0;
	background-color: #fff;
}
.rightbar .resume-item:last-child {
	border-bottom: 0px;
}
.rightbar .resume-title {
	font-size: 18px;
}
.rightbar .item-add {
	float: right;
	width: 22px;
	height: 22px;
	vertical-align: middle;
	background: url(/img/edit_ico_c.png) top left no-repeat transparent;
	background-position: 0 2px;
	display: inline-block;
	cursor: pointer;
}
.rightbar .edu-ico {
	width: 27px;
	margin-right: 10px;
	vertical-align: middle;
}
.rightbar .exp-ico {
	width: 21px;
	margin-right: 10px;
	vertical-align: -4px;	
}
.rightbar .pjt-ico {
	width: 22px;
	margin-right: 10px;
	vertical-align: -1px;	
}
.rightbar .link-ico {
	width: 21px;
	margin-right: 10px;
	vertical-align: middle;
}
.rightbar .ico-ui {
	width: 18px;
	margin-right: 10px;
	vertical-align: middle;
}
.rightbar .resume-init {
	height: 85px;
	color: #70a7df;
	font-size: 16px; 
	line-height: 85px;
	text-align: center;
	margin: 25px;
	border: 1px dashed #edeef0;
	cursor: pointer;
}
.rightbar .add-ico {
	width: 22px;
	height: 22px;
	background: url(/img/edit_ico_c.png) top left no-repeat transparent;
	display: inline-block;
	vertical-align: middle;
	margin-top: -5px;
	background-position: 0px 5px;
}
.rightbar .btn-wrap {
	height: 80px;
	line-height: 80px;
	text-align: center;
	background-color: #fff;
	padding-bottom: 20px;
}


/****** 文件上传******/
.upload-warp {
	width: 1024px;
	margin: 35px auto 0;
	border-radius: 3px;
	background-color: #fff;
}
.upload-warp .file-title {
	height: 50px;
	font-size: 16px;
	line-height: 50px;
	border-bottom: 1px solid #eeedf2;
	padding-left: 20px;
}
.upload-warp .file-title img {
	width: 16px;
	margin-right: 5px;
}
.upload-warp .file-con {
	padding: 30px 0;
}
.upload-warp .file-group {
	text-align: center;
	margin-bottom: 10px;
}
.upload-warp .file-con input {
	width: 440px;
	height: 48px;
	color: #494f53;
	font-size: 14px;
	text-indent: 10px;
	border-radius: 5px;
	background-color: #eceef1;
}
.upload-warp .slt-file {
	width: 100px;
	height: 48px;
	color: #fff;
	font-size: 16px;
	line-height: 48px;
	text-align: center;
	border-radius: 5px;
	background-color: #73a6e5;
	display: inline-block;
	cursor: pointer;
}
.upload-warp .file-group .hidden {
	height: 42px;
	width: 450px;
	display: none;
}
.upload-warp .file-tip {
	color: #84898c;
	padding-left: 242px;
	margin-bottom: 50px;
}
.upload-warp .file-btn-wrap {
	text-align: center;
	margin-bottom: 20px;
}
.upload-warp .t-a-c {
	text-align: center;
}
.upload-warp .dis {
	background-color: #ccc;
	cursor: default;
}


/****** 页尾******/
#footer {
	position: relative;
}

