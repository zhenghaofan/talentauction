}
.team-warp .cancel {
	margin-left: 40px;
}
.team-warp .team-con {
	padding: 20px 20px;
}
.team-warp .team-item {
	padding-bottom: 10px;
	border-bottom: 1px solid #eceef1;
	margin-bottom: 15px;
}
.team-warp .team-item:last-child {
	border-bottom: 0;
	margin-bottom: 0;
}
.team-warp .user-img {
	float: left;
	overflow: hidden;
	display: inline-block;
}
.team-warp .user-img img {
	width: 120px;
}
.team-warp .item-title {
	height: 30px;
