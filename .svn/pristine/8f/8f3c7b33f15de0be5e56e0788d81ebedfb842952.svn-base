}
.team-warp .canvas {
	width: 130px;
	height: 120px;
	line-height: 120px;
	text-align: center;
	overflow: hidden;
	border-radius: 3px 3px 0 0;
	background-color: #ECEDF1;
}
.team-warp .canvas img {
	width: 100px;
	vertical-align: middle;
	cursor: pointer;
}
.team-warp .prompt {
	width: 130px;
	height: 25px;
	color: #fff;
	line-height: 25px;
	text-align: center;
	border-radius: 0 0 3px 3px;
	margin-bottom: 20px;
	background-color: #C0BFC4;
}

/* 公司介绍 */
.intro-warp {
	overflow: hidden;
