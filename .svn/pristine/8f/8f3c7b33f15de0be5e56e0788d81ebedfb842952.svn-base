/* 右侧栏 */
.container {
	width: 747px;
	display: inline-block;
}
.container .panel {
	padding: 40px 45px 45px;
	border-radius: 0 0 5px 5px;
	background-color: #fff;
}
.container .panel-w {
	padding: 40px 0 45px 45px;
}
.container .canvas {
	height: 140px;
	width: 198px;
	line-height: 140px;
	text-align: center;
	overflow: hidden;
	border-radius: 5px 5px 0 0;
	background-color: #ECEDF1;
	cursor: pointer;
}
.container .canvas img {
	width: 120px;
	vertical-align: middle;
	cursor: pointer;
}
.container .prompt {
	height: 25px;
	width: 198px;
	color: #fff;
	line-height: 25px;
	text-align: center;
	border-radius: 0 0 5px 5px;
	background-color: #C0BFC4;
}
.container .size {
	height: 30px;
	color: #a1a6aa;
	font-size: 14px;
	line-height: 30px;
	padding-left: 45px;
}
.container .info-item {
	/* margin-top: 25px; */
}


/* 基本信息 */
.info .photo {
	float: left;
	height: 190px;
	width: 190px;
	margin-right: 45px;
}
.info .photo img {
	max-height: 190px;
	max-width: 190px;
}
.info .content {
	width: 460px;
	font-size: 16px;
	display: inline-block;
}
.info .content a {
	color: #4182c5;
}
.info .content div {
	min-height: 30px;
}
.info .content span {
	/* margin-right: 25px; */
}
.info .content .name {
	height: 45px;
}
.info .content .name span {
	margin-right: 10px;
}
.info .content .name span:first-child {
	font-size: 24px;
}
.info .content .url a {
	color: #7FC9FF;
}
.info-edit {
	font-size: 14px;
}
.info-edit .info-item.first {
	margin-top: 0;
	margin-bottom: 25px;
}
.info-edit .info-item > span {
	font-size: 18px;
	height: 40px;
	display: block;
}
.info-edit select, .info-edit input {
	width: 657px;
}
.info-edit input {
	width: 655px;
}
.info-edit .select-small {
	width: 315px;
}
.info-edit .select-small.mgn-right {
	margin-right: 22px;
}

.info-edit .phase-item.slt {
  border-color: #71A8E0;
  background: url(../img/icon-check-c.png) 101% -1% no-repeat;
}
.info-edit .phase-item {
  width: 77px;
  height: 77px;
  line-height: 77px;
  text-align: center;
  border-radius: 5px;
  border: 2px solid #fff;
  margin: 0 5px 5px 0;
  background-color: #eceef1;
  display: inline-block;
  cursor: pointer;
}

.info-edit .item-ori-input {
	position: relative;
}
.info-edit .item-ori-input-box {
	position: absolute;
	top: 6px;
	left: 5px;	
}
.info-edit .item-ori-input-box span {
	height: 22px;
	color: #fff;
	line-height: 22px;
	margin: 0 0 0 5px;
	padding: 4px 5px;
	border-radius: 5px;
	display: inline-block;
	background-color: #71a8e0;
	cursor: pointer;
}
.info-edit .item-ori-input-box span:after {
	content: 'x';
	margin-left: 5px;	
}
.info-edit .item-tip-s {
	margin: 5px 0;
	color: #a1a6aa;	
}
input[type="file"] {
	display: none;
}
.err-tip {
	color: red;
}
.info .weal em {
	border: 1px solid #ccc;
	font-size: 13px;
	padding: 3px 5px;
	margin-right: 10px;
	color: #485053;
}
.info .weal span {
	color: #a1a6aa;
	margin-right: 0;
}
.other em, .url em {
	color: #a1a6aa;
}
.other-span {
	display: inline-block;
	width: 150px;
	vertical-align: top;
}
.info .content .addr span{ 
	margin-right: 0;
	line-height: 24px;
}


/* 公司产品 */
.products {
	margin: 30px 0;
	overflow: hidden;
}
.products .products-new {
	text-align: center;
	padding: 30px 0;
	border: 1px dashed #ccc;
}
.products .products-new span {
	color: #71a8e0;
	display: inline-block;
	padding-left: 18px;
	background: url("../img/icon-add.png") no-repeat;
	background-position: 0 50%;
	cursor: pointer;
}
.products .photo {
	float: left;
	width: 320px;
	margin-right: 20px;
}
.products .photo img {
	max-width: 320px;
	max-height: 240px;
}
.products .repeat {
	padding-bottom: 25px;
	margin-bottom: 25px;
	border-bottom: 1px solid #eceef1;
	overflow: hidden;
}
.products .repeat:last-child {
	border-bottom: none;
	padding-bottom: 0;
	margin-bottom: 0;
}
.products .name a {
	color: #5c6469;
	text-decoration: underline;
	font-size: 18px;
}
.products .progress {
	font-size: 14px;
}
.products .content {
	width: 310px;
	font-size: 16px;
	display: inline-block;
}
.products .other pre {
	margin-top: 0;
}
.products .content div {
	min-height: 30px;
}
.products .content span {
	font-weight: bold;
}
.products .content .url a {
	color: #3FA9F5;
	font-weight: normal;
}
.products .content pre {
	line-height: 24px;
}
.products .content img {
	float: right;
	margin-left: 6px;
	cursor: pointer;
}
.products-edit {
	font-size: 14px;
}
.products-edit .info-item.first {
	margin-top: 0;
	margin-bottom: 5px;
}
.products-edit input {
	width: 655px;
}
.products-edit textarea {
	padding: 15px;
	height: 120px;	
	width: 626px;
}
.products-edit .phase-item {
	height: 48px;
	width: 106px;
	line-height: 48px;
	text-align: center;
	border-radius: 5px;
	border: 2px solid #fff;
	margin: 0 5px 5px 0;
	background-color: #eceef1;
	display: inline-block;
	cursor: pointer;
}
.products-edit .phase-item.slt {
	border-color: #71A8E0;
	background: url(../img/icon-check-c.png) 101% -1% no-repeat;
}



.qrcode-box-n {
	margin: 20px 0;
	padding: 20px;
}

.qrcode-img-n {
	width: 200px;
}


