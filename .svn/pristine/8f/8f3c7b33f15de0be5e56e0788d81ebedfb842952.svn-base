

.main-wrap {
	width: 1024px;
	color: #6B7485;
	margin: 130px auto 100px;
}
.main-wrap .sidebar {
	float: left;
	min-height: 500px;
	width: 245px;
	margin-right: 30px;
	display: inline-block;	
}
.main-wrap input, select {
	color: #6B7485;
	font-size: 14px;
	text-indent: 12px;
	border-radius: 3px;
	border: 1px solid #F3F1F1;
	background-color: #eceef1;
	display: inline-block;
}
.main-wrap input {
	height: 42px;
}
.main-wrap select {
	height: 44px;
	text-indent: 0;
}
.main-wrap textarea {
	color: #6B7485;
	resize: none;
	background-color: #ECEDF1;
}
.main-wrap .btn {
	color: #fff;
	border: none;
	border-radius: 3px;
	background-color: #71A8E0;	
}
.main-wrap .save {
	width: 80px;
	height: 40px;
}
.main-wrap .save:hover {
	background-color: #4182c6;
}
.main-wrap .cancel {
	width: 80px;
	height: 40px;
	color: #6B7485;
	border: 1px solid #ccc;
	background-color: #fff;
}
.main-wrap .cancel:hover {
	background-color: #E4E4E4;
}
.main-wrap .title {
	height: 50px;
	font-size: 16px;
	line-height: 50px;
	padding: 0 18px;
	border-bottom: 1px solid #eceef1;	
}
.main-wrap .new-box {
	text-align: center;
	padding: 30px 0;
	border: 1px dashed #ccc;
}
.main-wrap .new-box span {
	color: #71a8e0;
	padding-left: 18px;
	background: url("../img/icon-add.png") no-repeat;
	background-position: 0 50%;
	display: inline-block;
	cursor: pointer;
}
.main-wrap .opt-ico {
	float: right;
	height: 20px;
	margin-top: 5px;
	cursor: pointer;
}
.main-wrap .opt-ico img {
	width: 16px;
}
.main-wrap .add-ico {
	float: right;
	margin-top: 15px;
	display: inline-block;
	cursor: pointer;	
}
.main-wrap .earth {
	width:14px;
	vertical-align: middle;
}
.main-wrap .remarks {
	font-size: 16px;
	margin-bottom: 10px;
	display: block;
}
.main-wrap .err-wrap {
	min-height: 25px;
}
.main-wrap .err-wrap label {
	width: 260px;
	height: 25px;
	color: #FF1414;
	line-height: 25px;
	text-indent: 20px;
	padding-left: 18px;
	display: none;
}
.main-wrap .err-wrap .ico-invalid {
	background: url(../img/icon-error.png) 0 45% no-repeat;	
}



/* 信息完整度*/
.prog-warp {
	overflow: hidden;
	border-radius: 3px;
	margin-bottom: 20px;
	background-color: #fff;
}
.prog-warp .jump-btn {
	width: 210px;
	height: 50px;
	color: #fff;
	font-size: 16px;
	line-height: 50px;
	text-align: center;
	margin: 15px auto 0 auto;
	border-radius: 3px;
	background-color: #71a8e0;
	display: block;
}
.prog-warp .jump-btn:hover {
	background-color: #4182c6;
}
.prog-warp .prog-title {
	height: 50px;
	font-size: 16px;
	line-height: 50px;
	padding: 0 16px;
	border: none;	
}
.prog-warp .prog-con {
	padding: 0 20px 20px;
}
.prog-warp .prog-con > span:last-child {
	border-radius: 25px;
	background-color: #EDEEF0;
	display: block;	
}
.prog-warp i {
	width: 0%;
	height: 8px;
	margin-top: 10px;
	border-radius: 25px;
	background-color: #4182C6;
	display: block;	
}

/* 人才需求*/
.jd-warp {
	overflow: hidden;
	border-radius: 3px;
	margin-bottom: 20px;
	background-color: #fff;	
}
.jd-warp input {
	width: 203px;
}
.jd-warp textarea {
	width: 173px;
	height: 80px;
	padding: 10px 15px;
}
.jd-warp .cancel {
	margin-left: 40px;
}
.jd-warp .jd-title {
	height: 50px;
	font-size: 16px;
	line-height: 50px;
	padding: 0 18px;
	border-bottom: 1px solid #eceef1;
}
.jd-warp .jd-con {
	padding: 20px 20px;
}
.jd-warp .jd-item {
	height: 35px;
	line-height: 35px;
	border-bottom: 1px dashed #ccc;
}

/* 创始团队 */
.team-warp {
	overflow: hidden;
	border-radius: 3px;
	margin-bottom: 20px;
	background-color: #fff;		
}
.team-warp input {
	width: 203px;
}
.team-warp textarea {
	width: 173px;
	height: 120px;
	padding: 10px 15px;
}
.team-warp textarea.small {
