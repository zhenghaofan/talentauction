/****** 教育经历******/
.resume-box {
	font-size: 16px;
	margin: 20px 10px;
}
.resume-box a {
	color: #6B7485;
	word-break: break-all;
}
.resume-box a:hover {
	color: #4182c6;
}
.resume-box ul {
	border-bottom: 1px dashed #ccc;
	margin-bottom: 15px;
}
.resume-box ul:last-child {
	border-bottom: 0;
	margin-bottom: 0;
}
.resume-box li {
	line-height: 30px;
	margin-bottom: 5px;
}



/****** 操作按钮******/
.opt-btn-con {
	 vertical-align: middle;
	 margin-top: -6px;
	 display: none;
}
.opt-btn-con .del-btn {
	float: right;
	width: 22px;
	height: 22px;
	vertical-align: middle;
	background: url(/img/edit_ico_c.png) 0 -37px no-repeat;
	display: inline-block;
	cursor: pointer;
}
.opt-btn-con .edit-btn {
	float: right;
	width: 22px;
	height: 22px;
	vertical-align: middle;
	background: url(/img/edit_ico_c.png) 0 -16px no-repeat;
	display: inline-block;
	cursor: pointer;
}


/****** 编辑器******/
.box-edit {
	padding: 30px 120px;
	border-radius: 3px;
	margin-top: 12px;
	background: #edeef2;
}
.box-edit input[type="text"], .edit-box select,
.box-edit textarea {
	width: 425px;
	height: 38px;
	color: #6B7485;
	font-size: 14px;
	text-indent: 12px;
	border-radius: 3px;
	border: 1px solid #F3F1F1;
	display: inline-block;
	background-color: #fff;
}
.box-edit select {
	height: 40px;
	text-indent: 0;
}
.box-edit textarea {
	width: 405px;
	height: 100px;
	text-indent: 0;
	padding: 10px;
	resize: none;
}
.box-edit input.min {
	width: 199px;
}
.box-edit .remarks {
	color: #6B7485;
	font-size: 16px;
	margin-bottom: 8px;
	display: block;
}
.box-edit .err-wrap {
	min-height: 25px;
}
.box-edit .err-wrap label {
	height: 25px;
	color: #FF1414;
	line-height: 25px;
	text-indent: 20px;
	padding-left: 18px;
	display: none;
}
.box-edit .err-wrap .ico-invalid {
	background: url(../img/icon-error.png) 0 45% no-repeat;
}
.box-edit .btn-con .btn {
	height: 32px;
	margin-right: 20px;
}
.box-edit .btn-con span {
	cursor: pointer;
}


/****** 文件上传******/
.upload-warp {
	width: 1024px;
	margin: 35px auto 0;
	border-radius: 3px;
	background-color: #fff;
}
.upload-warp .file-title {
	height: 50px;
	font-size: 16px;
	line-height: 50px;
	border-bottom: 1px solid #eeedf2;
	padding-left: 20px;
}
.upload-warp .file-title img {
	width: 16px;
	margin-right: 5px;
}
.upload-warp .file-con {
	padding: 30px 0;
}
.upload-warp .file-group {
	text-align: center;
	margin-bottom: 10px;
}
.upload-warp .file-con input {
	width: 440px;
	height: 48px;
	color: #6B7485;
	font-size: 14px;
	text-indent: 10px;
	border-radius: 3px;
	background-color: #eceef1;
}
.upload-warp .slt-file {
	width: 100px;
	height: 50px;
	color: #fff;
	font-size: 16px;
	line-height: 50px;
	text-align: center;
	vertical-align: -1px;
	border-radius: 3px;
	background-color: #73a6e5;
	display: inline-block;
	cursor: pointer;
}
.upload-warp .file-group .hidden {
	height: 42px;
	width: 450px;
	display: none;
}
.upload-warp .err-wrap {
	padding-left: 242px;
	margin-bottom: 30px;
}
.upload-warp .err-wrap label {
	height: 25px;
	color: #84898c;
	line-height: 25px;
	padding-left: 18px;
	background: url(../img/icon-error.png) 0 45% no-repeat;
	background-position: 0 45%;
}
.upload-warp .err-wrap .ico-invalid {
	color: #FF1414;
}
.upload-warp .file-btn-wrap {
	text-align: center;
	margin-bottom: 20px;
}
.upload-warp .btn {
	height: 45px;
	color: #fff;
	font-size: 14px;
	padding: 0 20px;
	border-radius: 3px;
	border: none;
	background-color: #73a6e5;
	cursor: pointer;
}
.upload-warp .btn:hover {
	background-color: #4182c6;
}
.upload-warp .btn.old {
	margin-right: 20px;
}
.upload-warp .btn.submit {
	width: 115px;
}
.upload-warp .btn.dis {
	background-color: #dadbe0;
}
.upload-warp .btn.dis:hover {
	background-color: #ced1d6;
}





/****** 基本信息填写弹出框*****/
.info-modal {
	display: none;
}
.info-modal .modal-dialog {
	width: 380px;
	margin-top: 10%;
}
.info-modal .modal-header {
	font-size: 18px;
	padding: 16px 25px;
	border-bottom: 1px solid #eceef1;
	background-size: 30px;
	background-position: 6px 50%;
}
.info-modal .modal-title {
	color: #6B7485;
	display: inline-block;
}
.info-modal .close {
	font-weight: normal;
	background-color: #fff;
}
.info-modal .modal-body {
	padding: 20px 50px 5px;
}
.info-modal .modal-footer {
	text-align: center;
	padding: 0 25px 30px;
	border-top: none;
}
.info-modal .btn.save {
	height: 38px;
	padding: 0 18px;
	border-radius: 3px;
}
.info-modal .btn.cancel {
	height: 38px;
	padding: 0 18px;
	border-radius: 3px;
}
.info-modal .btn.cancel:hover {
	background-color: #E4E4E4;
}
.info-modal .box-info input[type="text"], .box-info select {
	width: 276px;
	height: 38px;
	color: #6B7485;
	font-size: 14px;
	text-indent: 12px;
	border-radius: 3px;
	border: 1px solid #F3F1F1;
	display: inline-block;
	background-color: #eceef1;
}
.info-modal .box-info select {
	height: 40px;
	text-indent: 0;
}
.info-modal .box-info input.min {
	width: 96px;
}
.info-modal .box-info .err-wrap {
	min-height: 25px;
}
.info-modal .box-info .err-wrap label {
	height: 25px;
	color: #FF1414;
	line-height: 25px;
	text-indent: 20px;
	padding-left: 18px;
	display: none;
}
.info-modal .box-info .err-wrap .ico-invalid {
	background: url(../img/icon-error.png) 0 45% no-repeat;
}
.info-modal .box-info .remarks {
	color: #6B7485;
	font-size: 14px;
	margin-bottom: 8px;
	display: block;
}
.info-modal .sex-item {
	width: 79px;
	height: 40px;
	text-align: center;
	line-height: 40px;
	border-radius: 3px;
	margin-left: 6px;
	background-color: #ECEEF1;
	display: inline-block;
}
.info-modal .sex-text {
	margin-left: 10px;
}


