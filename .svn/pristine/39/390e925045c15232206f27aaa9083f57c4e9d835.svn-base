.body-wrap {
	margin: 30px 0 100px;
	background-color: #fff;
}
.body-wrap .btn, .info-title .btn {
	height: 45px;
	color: #fff;
	font-size: 14px;
	padding: 0 20px;
	border-radius: 3px;
	border: none;
	background-color: #73a6e5;
	cursor: pointer;
}
.body-wrap .btn:hover, .info-title .btn:hover {
	background-color: #4182c6;
}
.body-wrap .btn.old,  .info-title .btn.old {
	margin-right: 20px;
}
.body-wrap .btn.submit, .info-title .btn.submit {
	width: 115px;
}
.body-wrap .btn.dis .info-title .btn.dis {
	background-color: #dadbe0;
}
.body-wrap .btn.dis:hover .info-title .btn.dis:hover {
	background-color: #ced1d6;
}


.info-warp {
	width: 1024px;
	margin: 130px auto 100px;
	border-radius: 3px;
	color: #6B7485;	
}
.info-title {
	text-align: center;
	padding: 35px 0 50px;
	border-radius: 3px;
	background-color: #fff;
}

/****** 当前步骤******/
.info-title .tab-item {
	width: 60px;
	text-align: center;
	vertical-align: top;
	margin-left: -20px;
	margin-top: 3px;
	display: inline-block;
}
.info-title .tab-item:first-child {
	margin-left: 0;
}
.info-title .tab-item-step {
	color: #fff;
	width: 20px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	font-size: 14px;
	margin-bottom: 5px;
	border-radius: 22px;
	border: 1px solid #d8dbe0;
	background-color: #d8dbe0;	
	display: inline-block;
}
.info-title .tab-item-text {
	color: #d8dbe0;
}
.info-title .tab-item-slt {
	width: 64px;
	margin-top: 0;
}
.info-title .tab-item-slt .tab-item-step {
	width: 25px;
	height: 25px;
	color: #fff;
	font-size: 15px;
	line-height: 25px;
	text-align: center;
	margin-bottom: 10px;
	border-radius: 15px;
	border: 1px solid #70a7e0;
	background-color: #70a7e0;
	display: inline-block;
}
.info-title .tab-item-slt .tab-item-text {
	color: #6B7485;
	font-size: 15px;
}
.info-title .tab-line {
	width: 115px;
	height: 7px;
	vertical-align: top;
	margin-top: 10px;
	margin-left: -20px;
	background-color: #d8dbe0;
	display: inline-block;
}
.info-title .tip-wrap {
	height: 60px;
	line-height: 60px;
	text-align: center;
	background-position: 50px 50%;
}
.info-title .tip-wrap label {
	color: #4884c2;
}
.info-title .tip-wrap .ico {
	width: 14px;
	height: 14px;
	vertical-align: middle;
	margin-top: -4px;
	background: url(/img/icon-error-grey.png) top left no-repeat transparent;
	display: inline-block;
}
.info-title .slt-wrap .btn {
	width: 100px;
	height: 45px;
	color: #fff;
	line-height: 45px;
	border-radius: 3px;
	background-color: #71a8e0;
	display: inline-block;
	cursor: pointer;
}
.info-title .slt-wrap .btn:first-child {
	margin-right: 50px;
}
.info-title .slt-wrap .btn.slt {
	background-color: #4182c6;
	cursor: auto;
}
.info-title .btn .rand {
	width: 17px;
	height: 17px;
	vertical-align: middle;
	margin-top: -4px;
	border-radius: 8px;
	background-color: #fff;
	display: inline-block;
}
.info-title .slt .rand {
	background: url(/img/w_gou.png) center center no-repeat #fff;
}



/****** 左侧栏******/
