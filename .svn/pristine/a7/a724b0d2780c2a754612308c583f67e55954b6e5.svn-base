	margin: 0 10px 10px 0;
	display: inline-block;
}
.base-wrap .cancel {
	margin-left: 40px;
}
.base-wrap .base-con {
	padding: 40px 45px 45px;
}
.base-wrap .base-con.pdg {
	padding: 40px 0 45px 45px;
}
.base-wrap .photo {
	float: left;
	width: 190px;
	height: 190px;
	overflow: hidden;
	margin-right: 45px;
}
.base-wrap .photo img {
	width: 190px;
}
.base-wrap .base-info {
	width: 460px;
	font-size: 16px;
	display: inline-block;
}
.base-wrap .base-info a {
	color: #4182c5;
}
.base-wrap .base-info div {
	min-height: 30px;
}
.base-wrap .base-info .name {
	height: 45px;
}
.base-wrap .base-ico {
	float: none;
	max-width: 18px;
	max-height: 18px;
	margin-right: 5px;
	margin-bottom: -2px;
}
.base-wrap .edit-box {
	font-size: 14px;
}
.base-wrap input {
	width: 655px;
}
.base-wrap select {
	width: 657px;
}
.base-wrap select.small {
	width: 315px;
}
.base-wrap select.mgn-1 {
	margin-right: 22px;
}
.base-wrap .welfare-tip {
	margin: 13px 0 10px;
}
.base-wrap .phase-item {
	width: 77px;
	height: 77px;
	line-height: 77px;
	text-align: center;
	border-radius: 5px;
	border: 2px solid #fff;
	margin: 0 5px 5px 0;
	background-color: #eceef1;
	display: inline-block;
	cursor: pointer;
}
.base-wrap .phase-item.slt {
	border-color: #71A8E0;
	background: url(../img/icon-check-c.png) 101% -1% no-repeat;
}

/* 公司产品 */
.pdt-wrap {
	overflow: hidden;
	border-radius: 3px;
	margin-bottom: 20px;
	background-color: #fff;	
}
.pdt-wrap input {
	width: 655px;
}
.pdt-wrap textarea {
	padding: 15px;
	height: 120px;	
	width: 626px;
}
.pdt-wrap .cancel {
	margin-left: 40px;
}
.pdt-wrap .pdt-con {
	padding: 40px 45px 45px;
}
.pdt-wrap .pdt-item {
	overflow: hidden;	
	padding-bottom: 25px;
